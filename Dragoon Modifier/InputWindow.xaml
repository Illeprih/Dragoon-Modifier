<Window x:Class="Dragoon_Modifier.InputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dragoon_Modifier"
        mc:Ignorable="d"
        Title="InputWindow" Width="400" MinWidth="400"
        ResizeMode="NoResize"
        SizeToContent="WidthAndHeight"
        FontSize="18">
    <Grid>
        <DockPanel>
            <StackPanel x:Name="pnlMain">
                <Button x:Name="btnOk" Content="Ok" Width="Auto" Click="btnOk_Click" IsDefault="True"/>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
